    <rule>
        <name>SCDO_getCentroCostoByFilters</name>
        <uuid>_a-0000e8c4-62ba-8000-e603-01ef9001ef90_2682345</uuid>
        <description>Regla que consulta todos los datos del CDT Centro de Costo</description>
        <parentUuid>_a-0000e9a7-6d42-8000-e63f-01ef9001ef90_4873693</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition> #"SYSTEM_SYSRULES_queryEntity_v2"(
   entity:  #"_a-0000e8b2-d897-8000-e5f1-01ef9001ef90_2379550",
   query: #"SYSTEM_SYSRULES_query"(
     logicalExpression: #"SYSTEM_SYSRULES_queryLogicalExpression"(
       operator: "AND",
       filters: {
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idccostos",
      operator: "=",
      value: ri!idccostos,
      applyWhen: isnull(ri!idccostos) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "cc",
      operator: "=",
      value: ri!cc,
      applyWhen: isnull(ri!cc) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idsegmento",
      operator: "=",
      value: ri!idsegmento,
      applyWhen: isnull(ri!idsegmento) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idregion",
      operator: "=",
      value: ri!idregion,
      applyWhen: isnull(ri!idregion) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "nombre",
      operator: "=",
      value: ri!nombre,
      applyWhen: isnull(ri!nombre) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idzona",
      operator: "=",
      value: ri!idzona,
      applyWhen: isnull(ri!idzona) &lt;&gt; true
    )
  },
  
  ignoreFiltersWithEmptyValues: true()
     ),
     pagingInfo: if(
       a!isNotNullOrEmpty(ri!paginInfo),
       ri!paginInfo,
       #"SYSTEM_SYSRULES_pagingInfo"(
         startIndex: 1,
         batchSize: 2000,
       )
     )
 )
)




</definition>
