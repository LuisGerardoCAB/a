        <uuid>_a-0000e982-d80b-8000-e63b-01ef9001ef90_4746884</uuid>
        <description>Regla de expresi√≥n que devuelve un administrador dado un usuario logueado</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4831457</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
