<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000ec5b-dce3-8000-e69f-01ef9001ef90_10103200</versionUuid>
    <rule>
        <name>SCDO_CREAR_DOC_FISICOS_PARA_INSERTAR</name>
        <uuid>_a-0000eaa5-8248-8000-e681-01ef9001ef90_8239803</uuid>
        <description>Regla de expresión para insertar doc físicos en la bd</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4829216</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!doc_insertar:if(a!isNotNullOrEmpty(ri!contratos),
  #"SYSTEM_SYSRULES_forEach"(
    items:ri!contratos,
    expression :{
      'type!{urn:com:appian:types}cdo_doctos_fisicos'(
        
        contrato:fv!item,
        fechaRegistro:now(),
        adf:ri!num_adf[fv!index],
        idusuario:ri!username_usuario,
        comentarios:null,
        iddocumento:ri!iddocumento,
        estatus:1,
                       
      )
    }
  )
  ,null),
  
  local!doc_insertar
)</definition>
        <namedTypedValue>
            <name>contratos</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>username_usuario</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>num_adf</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>iddocumento</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
