    <rule>
        <name>SCDO_ObtenerDocumentacionRechazada</name>
        <uuid>_a-0000e982-d80b-8000-e63b-01ef9001ef90_4763710</uuid>
        <description>Regla para obtener la documentacion rechazada</description>
        <parentUuid>_a-0000e982-d80b-8000-e63b-01ef9001ef90_4815880</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_queryEntity_v2"(
  entity: #"_a-0000e982-d80b-8000-e63b-01ef9001ef90_4749061",
  query: #"SYSTEM_SYSRULES_query"(
    logicalExpression: #"SYSTEM_SYSRULES_queryLogicalExpression"(
      operator: "AND",
      filters: {
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "contrato",
          operator: "=",
          value: ri!contrato
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "iddocumento",
          operator: "=",
          value: ri!iddocumento
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "iddocumento",
          operator: "in",
          value: ri!idsDocumentos,
          applywhen: a!isNotNullOrEmpty(ri!idsDocumentos),
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "tipo",
          operator: "=",
          value: ri!tipo
        )
      },
      ignoreFiltersWithEmptyValues: true
    ),
    pagingInfo: if(
      a!isNullOrEmpty(ri!pagingInfo),
      #"SYSTEM_SYSRULES_pagingInfo"(startIndex: 1, batchSize: 10,
      /*sort: a!sortInfo(field: "iddocumentacionrechazada", ascending: false)*/
      ),
      ri!pagingInfo
    )
  ),
  fetchTotalCount: false
).data</definition>
        <namedTypedValue>
            <name>contrato</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>iddocumento</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>pagingInfo</name>
            <type>
                <name>PagingInfo</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>tipo</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>idsDocumentos</name>
            <type>
                <name>Integer?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
