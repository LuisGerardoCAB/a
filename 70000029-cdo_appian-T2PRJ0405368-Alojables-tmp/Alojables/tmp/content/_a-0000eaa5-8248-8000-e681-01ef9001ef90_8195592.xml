    <rule>
        <name>SCDO_ACTUALIZAR_DOCS_FISICOS_A_RECHAZAR</name>
        <uuid>_a-0000eaa5-8248-8000-e681-01ef9001ef90_8195592</uuid>
        <description>Regla de expresión para actualizar los documentos físicos relacionados con la documentación a rechazar.</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4829216</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!docs:ri!docs_fisicos,
  if(a!isNotNullOrEmpty(local!docs),
  #"SYSTEM_SYSRULES_forEach"(
    items:local!docs,
    expression: 'type!{urn:com:appian:types}cdo_doctos_fisicos'(
      iddoctosFisicos: fv!item.iddoctosFisicos,
      contrato:fv!item.contrato,
      fechaRegistro:fv!item.fechaRegistro,
      adf:fv!item.adf,
      idusuario:fv!item.idusuario,
      comentarios:concat("invalidado por rechazo de documento",now(),"por usuario",fv!item.idusuario),
      iddocumento:fv!item.iddocumento,
      estatus:"0"
    )
  )
  
  
  ,null
  )
)</definition>
        <namedTypedValue>
            <name>docs_fisicos</name>
            <type>
                <name>cdo_doctos_fisicos?list</name>
                <namespace>urn:com:appian:types</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
