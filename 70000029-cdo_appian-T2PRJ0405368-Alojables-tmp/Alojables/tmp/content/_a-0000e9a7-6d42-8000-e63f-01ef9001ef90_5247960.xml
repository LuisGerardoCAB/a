            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition> #"SYSTEM_SYSRULES_queryEntity_v2"(
   entity: #"_a-0000e9a7-6d42-8000-e63f-01ef9001ef90_5247914",
   query: #"SYSTEM_SYSRULES_query"(
     logicalExpression: #"SYSTEM_SYSRULES_queryLogicalExpression"(
       operator: "AND",
       filters: {
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idtabla",
      operator: "=",
      value: ri!idTabla_txt,
      applyWhen: isnull(ri!idTabla_txt) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "codigollave",
      operator: "=",
      value: ri!codigoLlave_txt,
      applyWhen: isnull(ri!codigoLlave_txt) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "fechaYHora",
      operator: "between",
      value: {
        datetime(
          year(ri!fechaInicio_dt),
          month(ri!fechaInicio_dt),
          day(ri!fechaInicio_dt),
          0,
          0,
          0
        ) - intervalds(timezone() / 60, 0, 0),
        datetime(
          year(ri!fechaFin_dt),
          month(ri!fechaFin_dt),
          day(ri!fechaFin_dt),
          23,
          59,
          59
        ) - intervalds(timezone() / 60, 0, 0),
      },
      applyWhen: and(
        not(#"_a-0000e867-3fd5-8000-e5e3-01ef9001ef90_1882382"(ri!fechaInicio_dt)),
        not(#"_a-0000e867-3fd5-8000-e5e3-01ef9001ef90_1882382"(ri!fechaFin_dt))
      )
    )
  },
  ignoreFiltersWithEmptyValues: true()
     ),
     pagingInfo: if(
       a!isNotNullOrEmpty(ri!paginInfo_pi),
       ri!paginInfo_pi,
       #"SYSTEM_SYSRULES_pagingInfo"(
         startIndex: 1,
         batchSize: 500,
       )
     )

   )
 )
</definition>
