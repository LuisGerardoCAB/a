        <name>SCDO_obtenerLogModificacionesDiarias</name>
        <uuid>_a-0000e9a7-6d42-8000-e63f-01ef9001ef90_5247960</uuid>
        <description>Consulta informacion de la tabla CDO_MX_AUX_MODIFICACIONES_RA</description>
        <parentUuid>_a-0000e867-3fd5-8000-e5e3-01ef9001ef90_1882376</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
