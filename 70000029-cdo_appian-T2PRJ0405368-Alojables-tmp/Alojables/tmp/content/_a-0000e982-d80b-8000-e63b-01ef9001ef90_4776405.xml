        <uuid>_a-0000e982-d80b-8000-e63b-01ef9001ef90_4776405</uuid>
        <description>Regla para obtener los digitalizados rechazados</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4831345</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_queryEntity_v2"(
  entity: #"_a-0000e982-d80b-8000-e63b-01ef9001ef90_4776591",
  query: #"SYSTEM_SYSRULES_query"(
    logicalExpression: #"SYSTEM_SYSRULES_queryLogicalExpression"(
      operator: "AND",
      filters: {
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "idreporte",
          operator: "=",
          value: ri!idreporte,
          applyWhen: isnull(ri!idreporte) &lt;&gt; true
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "operacion",
          operator: "=",
          value: ri!operacion
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "contrato",
          operator: "=",
          value: ri!contrato
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "iddocumento",
          operator: "=",
          value: ri!iddocumento
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "iddocumento",
          operator: "in",
          value: ri!idsDocumentos,
          applywhen: a!isNotNullOrEmpty(ri!idsDocumentos)
        )
      },
      ignoreFiltersWithEmptyValues: true
    ),
    pagingInfo: if(
      a!isNullOrEmpty(ri!pagingInfo),
      #"SYSTEM_SYSRULES_pagingInfo"(
        startIndex: 1,
        batchSize: 10
      ),
      ri!pagingInfo
    )
  ),
  fetchTotalCount: false
).data</definition>
