    <versionUuid>_a-0000eb53-0a0e-8000-e689-01ef9001ef90_9385750</versionUuid>
    <rule>
        <name>SCDO_ACTUALIZAR_DOC_FISICOS_PARA_REGISTRAR_ARRIBO</name>
        <uuid>_a-0000eaa5-8248-8000-e681-01ef9001ef90_8209518</uuid>
        <description>Regla de expresión para actualizar doc físicos en la bd</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4829216</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!doc_act:if(a!isNotNullOrEmpty(ri!doc_fisicos),
  #"SYSTEM_SYSRULES_forEach"(
    items:ri!doc_fisicos,
    expression :{
      'type!{urn:com:appian:types}cdo_doctos_fisicos'(
        iddoctosFisicos: fv!item.iddoctosFisicos,
        contrato:fv!item.contrato,
        fechaRegistro:fv!item.fechaRegistro,
        adf:fv!item.adf,
        /*adf:ri!adf,*/
        idusuario:fv!item.iddoctosFisicos,
        comentarios:concat("Invalidado por nuevo ADF ",now(), "por usuario ",ri!username_usuario),
        iddocumento:fv!item.iddocumento,
        estatus:0,
       
                
      )
    }
  )
  ,null),
  
  local!doc_act
)</definition>
        <namedTypedValue>
            <name>doc_fisicos</name>
            <type>
                <name>cdo_doctos_fisicos?list</name>
                <namespace>urn:com:appian:types</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>username_usuario</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
