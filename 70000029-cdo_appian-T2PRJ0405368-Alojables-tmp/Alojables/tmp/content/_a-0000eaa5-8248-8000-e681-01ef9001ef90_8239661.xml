    <rule>
        <name>SCDO_OBTENER_DOCUMENTOS_FISICOS_SEGUN_CONTRATOS</name>
        <uuid>_a-0000eaa5-8248-8000-e681-01ef9001ef90_8239661</uuid>
        <description>Obtiene todos los documentos fisicos seg√∫n lista de contratos</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4829216</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!estan: if(
    a!isNotNullOrEmpty(ri!contratos),
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!contratos,
      expression: if(
        a!isNotNullOrEmpty(
          #"_a-0000e982-d80b-8000-e63b-01ef9001ef90_4749073"(iddocumento:ri!iddocumento,estatus:1,contrato:fv!item)
        ),
        #"_a-0000e982-d80b-8000-e63b-01ef9001ef90_4749073"(iddocumento:ri!iddocumento,estatus:1,contrato:fv!item),
        null
      )
    ),
    null
  ),
  local!devolver: reject(fn!isnull, a!flatten(local!estan)),
  local!devolver


)</definition>
        <namedTypedValue>
            <name>contratos</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>iddocumento</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
