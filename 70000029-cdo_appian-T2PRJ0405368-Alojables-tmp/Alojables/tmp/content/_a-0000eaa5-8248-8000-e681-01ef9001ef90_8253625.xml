        <definition>a!localVariables(
  local!datos:#"SYSTEM_SYSRULES_forEach"(
    items: ri!ids_documento,
    expression: 'type!{urn:com:appian:types}scdo_documentacion'(
      contrato:ri!contrato,
      nomdocumento:ri!nomdocumento,/*document(ri!documento,"name")ri!nomdocumento*/
      /*concat(*/
        /*ri!contrato,*/
        /*"_",*/
        /*ri!usuario,*/
        /*"_idDocumento",*/
        /*"_",*/
        /*fv!item,*/
        /*"_",*/
        /*text(now(), "yyyy-MM-dd hh:mm:ss")*/
      /*),*/
      /*concat(ri!contrato,"_",ri!usuario,"_",text(now(), "yyyy-MM-dd hh:mm:ss")),*/
      iddocumento: fv!item,
      tipo: "CONT",
      fechacarga: text(now(), "yyyy-MM-dd hh:mm:ss"),
      usuariocargo: ri!usuario,
      cc:if(a!isNotNullOrEmpty(#"_a-0000e878-569a-8000-e5e4-01ef9001ef90_2152641"(usuarioOpic :ri!usuario).data),
      tostring(#"_a-0000e878-569a-8000-e5e4-01ef9001ef90_2152641"(usuarioOpic :ri!usuario).data.idsegmento),null),
      estado:"P",
      
    )
  ),
  local!datos
)</definition>
