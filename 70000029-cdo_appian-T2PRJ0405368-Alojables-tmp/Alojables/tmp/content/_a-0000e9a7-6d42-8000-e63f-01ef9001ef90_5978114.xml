<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000eb53-0a0e-8000-e689-01ef9001ef90_9504884</versionUuid>
    <rule>
        <name>SCDO_llenarDocumentoFisicoPorReportes</name>
        <uuid>_a-0000e9a7-6d42-8000-e63f-01ef9001ef90_5978114</uuid>
        <description>Regla de expresión que llena los documentos físicos dado reportes</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4829216</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!doctosfisicos: #"SYSTEM_SYSRULES_forEach"(
    items: if(a!isNullOrEmpty(ri!reportes), ri!reportes_aprobados, ri!reportes),
    expression: {
      'type!{urn:com:appian:types}cdo_doctos_fisicos'(
        contrato: fv!item.contrato,
        fechaRegistro: now(),
        adf: ri!numeroADF,
        idusuario:#"_a-0000eae4-5659-8000-e685-01ef9001ef90_8619011"(usuario :ri!usuario)[1].idadmon /*rule!SCDO_getUsuariosOPByFilters(usuarioOpic: (user(ri!usuario, "username"))).data[1].idUsuarioOpic*/,
        comentarios: "",
        estatus: 1,
        iddocumento: ri!id_documento
      )
    }
  ),
  local!doctosfisicos
)</definition>
        <namedTypedValue>
            <name>reportes</name>
            <type>
                <name>cdo_reportes</name>
                <namespace>urn:com:appian:types</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>reportes_aprobados</name>
            <type>
                <name>cdo_reportes_aprobados</name>
                <namespace>urn:com:appian:types</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>numeroADF</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>usuario</name>
            <type>
                <name>User</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>id_documento</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
