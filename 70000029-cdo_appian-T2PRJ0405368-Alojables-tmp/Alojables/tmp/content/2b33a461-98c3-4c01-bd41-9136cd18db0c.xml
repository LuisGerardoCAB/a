    <rule>
        <name>SCDO_obtenerDocumentosFisicosPorFiltrosReducidos</name>
        <uuid>2b33a461-98c3-4c01-bd41-9136cd18db0c</uuid>
        <description>Regla de expresión que devuelve los documentos físicos dados los siguientes parámetros:
contrato, adf, startIndex y batchSize</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4831029</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_queryEntity_v2"(
  entity: #"_a-0000e8b2-d897-8000-e5f1-01ef9001ef90_2373385",
  query: #"SYSTEM_SYSRULES_query"(
    logicalExpression: #"SYSTEM_SYSRULES_queryLogicalExpression"(
      operator: "AND",
      filters: {
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "iddoctosFisicos",
          operator: "includes",
          value: null
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "contrato",
          operator: "=",
          value: ri!contratoBuscar,

        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "adf",
          operator: "=",
          value: ri!adfBuscar,

        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "estatus",
          operator: "=",
          value: 1,

        ),

      },
      ignoreFiltersWithEmptyValues: true
    ),
    pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(
      startIndex: ri!startIndex,
      batchSize: ri!batchSize,
      sort: #"SYSTEM_SYSRULES_sortInfo"(field: "iddoctosFisicos", ascending: true())

    )
  ),
  fetchTotalCount: true
),</definition>
        <namedTypedValue>
            <name>contratoBuscar</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
