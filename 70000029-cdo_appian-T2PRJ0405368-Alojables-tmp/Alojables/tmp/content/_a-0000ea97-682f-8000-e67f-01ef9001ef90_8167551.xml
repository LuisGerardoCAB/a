    <rule>
        <name>SCDO_COMPROBAR_FORMATOS</name>
        <uuid>_a-0000ea97-682f-8000-e67f-01ef9001ef90_8167551</uuid>
        <description>Regla de expres√≥n para comprobar que el formato de un  documento sea .xlsx o .xls</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4829216</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  
  local!formato:if(or(like(upper(ri!texto),"*.XLSX"),like(upper(ri!texto),"*.XLS")),"excel",
  if(like(upper(ri!texto),"*.PDF"),"pdf",if(like(upper(ri!texto),"*.TXT"),"txt",if(or(like(upper(ri!texto),"*.PNG"),like(upper(ri!texto),"*.JPG")),"imagen",null)))),
  local!formato
  
)</definition>
        <namedTypedValue>
            <name>texto</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
