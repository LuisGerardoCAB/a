        <uuid>_a-0000eaa5-8248-8000-e681-01ef9001ef90_8334898</uuid>
        <description>Regla de expresión para obtener los digitalizados rechazados a eliminar según txt.</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4829216</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!eliminar:#"SYSTEM_SYSRULES_forEach"(
    items: ri!digitalizados,
    expression: {
      a!localVariables(
        local!auxiliar:#"_a-0000e982-d80b-8000-e63b-01ef9001ef90_4776405"(operacion:fv!item.operacion,iddocumento:fv!item.iddocumento ),
        if(a!isNotNullOrEmpty(local!auxiliar),
        local!auxiliar,
        null
        
        )
      )
    }
  ),
  local!devolver: a!flatten(reject(fn!isnull, local!eliminar)),
  local!devolver
)</definition>
        <namedTypedValue>
            <name>digitalizados</name>
            <type>
                <name>cdo_digitalizados?list</name>
                <namespace>urn:com:appian:types</namespace>
            </type>
        </namedTypedValue>
