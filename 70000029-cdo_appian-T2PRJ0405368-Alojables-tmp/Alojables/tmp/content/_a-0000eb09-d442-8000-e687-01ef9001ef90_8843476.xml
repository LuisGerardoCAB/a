        <name>SCDO_getReportesAprobadosByFilterCorreccion</name>
        <uuid>_a-0000eb09-d442-8000-e687-01ef9001ef90_8843476</uuid>
        <description>Regla que cosulta los datos al CDT cdo_reportes_aprobados para correccion</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4829472</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e8b9-0989-8000-e5fd-01ef9001ef90_2420859"(
  dataStoreEntity_dt: #"_a-0000e8d8-9b98-8000-e605-01ef9001ef90_2735521",
  queryFilter_any: {
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idreporte",
      operator: "=",
      value: ri!idreporte,
      applyWhen: isnull(ri!idreporte) &lt;&gt; true
    ),
   
   
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "operacion",
      operator: "=",
      value: ri!operacion,
      applyWhen: isnull(ri!operacion) &lt;&gt; true
    ),
  
 
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "usuario",
      operator: "in",
      value: ri!usuarios,
      applyWhen: isnull(ri!usuarios) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "ccostos",
      operator: "in",
      value: ri!ccostos,
      applyWhen: isnull(ri!ccostos) &lt;&gt; true
    ),
  
    
  },
  selectionColumn_txt: ri!seleccionColumnas,
  pagingInfo_pi: if(
    isnull(ri!paginInfo),
    #"SYSTEM_SYSRULES_pagingInfo"(startIndex: 1, batchSize: 50),
    ri!paginInfo
  ),
  ignoreFiltersWithEmptyValues_bool: true,
  fetchTotalCount_bool: true
).data
</definition>
