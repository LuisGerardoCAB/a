<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000ec5b-dce3-8000-e69f-01ef9001ef90_10114235</versionUuid>
    <rule>
        <name>SCDO_llenarDigitalizadosODocumentacionDadoReportes</name>
        <uuid>_a-0000e9a7-6d42-8000-e63f-01ef9001ef90_5932233</uuid>
        <description>Regla de expresi√≥n que permite guardar digitalizados dado una lista de reportes, ya sean aprobados o reportes en general</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4829216</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  
  local!digitalizadosODocumentacion: #"SYSTEM_SYSRULES_forEach"(
    items: if(a!isNullOrEmpty(ri!reportes), ri!reportesAprobados, ri!reportes),
    expression: {
      if(
        ri!esdigitalizado = true,
        'type!{urn:com:appian:types}cdo_digitalizados'(
          idreporte: fv!item.idreporte,
          contrato: fv!item.contrato,
          iddocumento: ri!idDocumento,
          nombreimagen: ri!nombreImagen,
          estado: if(a!isNullOrEmpty(ri!estado),"P", ri!estado),
          fechaacepto: "",
          usuarioacepto: "",
          operacion: fv!item.operacion,
        ),
        'type!{urn:com:appian:types}scdo_documentacion'(
          contrato: fv!item.contrato,
          nomdocumento: ri!nombreImagen,
          iddocumento: ri!idDocumento,
          tipo: "CONT",
          fechacarga: text(now(), "yyyy-MM-dd hh:mm:ss"),
          usuariocargo:
          if(a!isNotNullOrEmpty(#"_a-0000e878-569a-8000-e5e4-01ef9001ef90_2152641"(idUsuarioOpic: ri!usuarioCargo).data),
          #"_a-0000e878-569a-8000-e5e4-01ef9001ef90_2152641"(idUsuarioOpic: ri!usuarioCargo).data[1].usuarioopics,null
          ),
         
          cc: fv!item.ccostos,
          estado: if(a!isNullOrEmpty(ri!estado),"P", ri!estado),
          usuarioacepto: "",
          fechaacepto: "",
          observacion: "",
          br: "",
          opendate: "",
          trad: "",
          castdsc: "",
          profile: "",
          max: "",
          canal: "",
          cliente: fv!item.nombre,
          br2: "",
          buc: "",
         
        
        )
      )
      
    }
  ),
  local!digitalizadosODocumentacion
)</definition>
        <namedTypedValue>
            <name>reportes</name>
            <type>
                <name>cdo_reportes?list</name>
                <namespace>urn:com:appian:types</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>reportesAprobados</name>
            <type>
                <name>cdo_reportes_aprobados?list</name>
                <namespace>urn:com:appian:types</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>idDocumento</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>nombreImagen</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>esdigitalizado</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>usuarioCargo</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>estado</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
