    <rule>
        <name>SCDO_DocumentoFiltrodocumento</name>
        <uuid>_a-0000e8d8-9b98-8000-e605-01ef9001ef90_2694147</uuid>
        <description>Regla que filtra por el campo Documento al CDT Catalogo Documentos</description>
        <parentUuid>_a-0000e982-d80b-8000-e63b-01ef9001ef90_4816217</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_queryEntity_v2"(
  entity: #"_a-0000e8d8-9b98-8000-e605-01ef9001ef90_2694251",
  query: #"SYSTEM_SYSRULES_query"(
    logicalExpression: #"SYSTEM_SYSRULES_queryLogicalExpression"(
      operator: "AND",
      filters: {
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "iddocumento",
          operator: "=",
          value: ri!iddocumento,
          applyWhen: isnull(ri!iddocumento) &lt;&gt; true
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "documento",
          operator: "=",
          value: ri!documento,
          applyWhen: isnull(ri!documento) &lt;&gt; true
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "documentaa",
          operator: "=",
          value: ri!documentaa,
          applyWhen: isnull(ri!documentaa) &lt;&gt; true
        ),

        #"SYSTEM_SYSRULES_queryFilter"(
          field: "idsegmento",
          operator: "=",
          value: ri!idsegmento,
          applyWhen: isnull(ri!idsegmento) &lt;&gt; true
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "orden",
          operator: "=",
          value: ri!orden,
          applyWhen: isnull(ri!orden) &lt;&gt; true
        ),
        #"SYSTEM_SYSRULES_queryFilter"(
          field: "descripcion",
          operator: "=",
          value: ri!descripcion,
          applyWhen: isnull(ri!descripcion) &lt;&gt; true
        )
      }
    ),
    pagingInfo: if(a!isNullOrEmpty(ri!paginInfo), #"SYSTEM_SYSRULES_pagingInfo"(startIndex: 1, batchSize: 500), ri!paginInfo)    
  ),
  fetchTotalCount: true()
)
</definition>
