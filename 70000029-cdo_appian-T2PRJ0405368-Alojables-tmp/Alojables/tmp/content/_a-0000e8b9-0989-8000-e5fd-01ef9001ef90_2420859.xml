        </visibility>
        <definition>#"SYSTEM_SYSRULES_queryEntity_v2"(
  entity: ri!dataStoreEntity_dt,
  query: #"SYSTEM_SYSRULES_query"(
    selection: if(
      #"_a-0000e867-3fd5-8000-e5e3-01ef9001ef90_1882382"(ri!selectionColumn_txt),
      null,
      #"SYSTEM_SYSRULES_querySelection"(
        columns: #"SYSTEM_SYSRULES_forEach"(
          ri!selectionColumn_txt,
          #"SYSTEM_SYSRULES_queryColumn"(
            field: fv!item,
            alias: fv!item
          )
        )
      )
    ),
    logicalExpression: if(
      and(
        #"_a-0000e867-3fd5-8000-e5e3-01ef9001ef90_1882382"(ri!queryFilter_any),
        #"_a-0000e867-3fd5-8000-e5e3-01ef9001ef90_1882382"(ri!logicalExpression_any)
      ),
      null,
      #"SYSTEM_SYSRULES_queryLogicalExpression"(
        operator: "AND",
        logicalExpressions: if(#"_a-0000e867-3fd5-8000-e5e3-01ef9001ef90_1882382"(ri!logicalExpression_any),{},ri!logicalExpression_any),
        filters: if(#"_a-0000e867-3fd5-8000-e5e3-01ef9001ef90_1882382"(ri!queryFilter_any),{},ri!queryFilter_any),
        ignoreFiltersWithEmptyValues: ri!ignoreFiltersWithEmptyValues_bool
      )
    ),
    pagingInfo: if(
      isnull(ri!pagingInfo_pi),
      #"SYSTEM_SYSRULES_pagingInfo"(1,-1),
      ri!pagingInfo_pi
    ),
    aggregation: if(not(isnull(ri!aggregation_any)), ri!aggregation_any, null)
  ),
  fetchTotalCount: ri!fetchTotalCount_bool
)</definition>
