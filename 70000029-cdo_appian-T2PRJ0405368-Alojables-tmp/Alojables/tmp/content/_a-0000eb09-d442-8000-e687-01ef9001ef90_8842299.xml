        <definition>index(#"SYSTEM_SYSRULES_queryEntity_v2"(
  entity: #"_a-0000e878-569a-8000-e5e4-01ef9001ef90_1932909",
  query: #"SYSTEM_SYSRULES_query"(
    logicalExpression: #"SYSTEM_SYSRULES_queryLogicalExpression"(
      operator: "AND",
      filters: {
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idreporte",
      operator: "=",
      value: ri!idreporte,
      applyWhen: isnull(ri!idreporte) &lt;&gt; true
    ),
   
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "operacion",
      operator: "=",
      value: ri!operacion,
      applyWhen: isnull(ri!operacion) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "usuario",
      operator: "in",
      value: ri!usuarios,
      applyWhen: isnull(ri!usuarios) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "ccostos",
      operator: "in",
      value: ri!ccostos,
      applyWhen: isnull(ri!ccostos) &lt;&gt; true
    ),
   
  },
  ignoreFiltersWithEmptyValues: true()
    ),
    pagingInfo: if(
      a!isNotNullOrEmpty(ri!paginInfo),
      ri!paginInfo,
      #"SYSTEM_SYSRULES_pagingInfo"(
        startIndex: 1,
        batchSize: 10,
      )
    )

  )
), "data", null)</definition>
