    <rule>
        <name>SCDO_getCentroCostoByFiltersAS</name>
        <uuid>_a-0000e94c-78eb-8000-e637-01ef9001ef90_3874604</uuid>
        <description>Regla que consulta todos los datos del CDT Centro de Costo</description>
        <parentUuid>_a-0000e9a7-6d42-8000-e63f-01ef9001ef90_4873693</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e8b9-0989-8000-e5fd-01ef9001ef90_2420859"(
  dataStoreEntity_dt: #"_a-0000e8b2-d897-8000-e5f1-01ef9001ef90_2379550",
  queryFilter_any: {
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idccostos",
      operator: "=",
      value: ri!idccostos,
      applyWhen: isnull(ri!idccostos) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "cc",
      operator: "=",
      value: ri!cc,
      applyWhen: isnull(ri!cc) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idsegmento",
      operator: "=",
      value: ri!idsegmento,
      applyWhen: isnull(ri!idsegmento) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idregion",
      operator: "=",
      value: ri!idregion,
      applyWhen: isnull(ri!idregion) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "nombre",
      operator: "=",
      value: ri!nombre,
      applyWhen: isnull(ri!nombre) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idzona",
      operator: "=",
      value: ri!idzona,
      applyWhen: isnull(ri!idzona) &lt;&gt; true
    )
  },
  pagingInfo_pi: if(
    not(isnull(ri!paginInfo)),
    ri!paginInfo,
    #"SYSTEM_SYSRULES_pagingInfo"(
      startIndex: 1,
      batchSize: -1,
      sort: #"SYSTEM_SYSRULES_sortInfo"(field: "idccostos", ascending: true())
    )
  ),
 
  selectionColumn_txt: ri!seleccionColumnas,   
  /*pagingInfo_pi: ri!paginInfo,*/
  ignoreFiltersWithEmptyValues_bool:true,
  fetchTotalCount_bool: true()
)




</definition>
