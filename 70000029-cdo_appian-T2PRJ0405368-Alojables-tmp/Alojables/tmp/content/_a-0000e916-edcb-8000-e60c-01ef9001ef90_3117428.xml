    <rule>
        <name>SCDO_getDigitalizadosByFilter</name>
        <uuid>_a-0000e916-edcb-8000-e60c-01ef9001ef90_3117428</uuid>
        <description>Regla que cosulta los datos al CDT cdo_digitalizados</description>
        <parentUuid>_a-0000e9a7-6d42-8000-e63f-01ef9001ef90_4874327</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e8b9-0989-8000-e5fd-01ef9001ef90_2420859"(
  dataStoreEntity_dt: #"_a-0000e916-edcb-8000-e60c-01ef9001ef90_3117600",
  queryFilter_any: {
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "iddigitalizado",
      operator: "=",
      value: ri!iddigitalizado,
      applyWhen: isnull(ri!iddigitalizado) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "idreporte",
      operator: "=",
      value: ri!idreporte,
      applyWhen: isnull(ri!idreporte) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "contrato",
      operator: "=",
      value: ri!contrato,
      applyWhen: isnull(ri!contrato) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "iddocumento",
      operator: "=",
      value: ri!iddocumento,
      applyWhen: isnull(ri!iddocumento) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "iddocumento",
      operator: "in",
      value: ri!idsDocumentos,
      applywhen: a!isNotNullOrEmpty(ri!idsDocumentos)
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "nombreimagen",
      operator: "=",
      value: ri!nombreimagen,
      applyWhen: isnull(ri!nombreimagen) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "estado",
      operator: "=",
      value: ri!estado,
      applyWhen: isnull(ri!estado) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "fechaacepto",
      operator: "=",
      value: ri!fechaacepto,
      applyWhen: isnull(ri!fechaacepto) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "usuarioacepto",
      operator: "=",
      value: ri!usuarioacepto,
      applyWhen: isnull(ri!usuarioacepto) &lt;&gt; true
    ),
    #"SYSTEM_SYSRULES_queryFilter"(
      field: "operacion",
      operator: "=",
      value: ri!operacion,
      applyWhen: isnull(ri!operacion) &lt;&gt; true
    ),
  },
  selectionColumn_txt: ri!seleccionColumnas,
  pagingInfo_pi: if(
    isnull(ri!paginInfo),
    #"SYSTEM_SYSRULES_pagingInfo"(startIndex: 1, batchSize: 50),
    ri!paginInfo
  ),
  ignoreFiltersWithEmptyValues_bool: true,
  fetchTotalCount_bool: true
).data
</definition>
