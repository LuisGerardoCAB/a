            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!devolverMatch: a!match(
    value: and(
      a!isNullOrEmpty(ri!arreglo_1),
      a!isNullOrEmpty(ri!arreglo_2),
      a!isNullOrEmpty(ri!arreglo_3)
    ),
    equals: true(),
    then: null,
    whenTrue: and(
      a!isNullOrEmpty(ri!arreglo_1),
      a!isNullOrEmpty(ri!arreglo_2)
    ),
    then: ri!arreglo_3,
    whenTrue: and(
      a!isNullOrEmpty(ri!arreglo_1),
      a!isNullOrEmpty(ri!arreglo_3)
    ),
    then: ri!arreglo_2,
    whenTrue: and(
      a!isNullOrEmpty(ri!arreglo_2),
      a!isNullOrEmpty(ri!arreglo_3)
    ),
    then: ri!arreglo_1,
    whenTrue: and(
      a!isNullOrEmpty(ri!arreglo_1),
      and(
        a!isNotNullOrEmpty(ri!arreglo_2),
        a!isNotNullOrEmpty(ri!arreglo_3)
      )
    ),
    then: difference(ri!arreglo_2, ri!arreglo_3),
    whenTrue: and(
      a!isNullOrEmpty(ri!arreglo_2),
      and(
        a!isNotNullOrEmpty(ri!arreglo_1),
        a!isNotNullOrEmpty(ri!arreglo_3)
      )
    ),
    then: difference(ri!arreglo_1, ri!arreglo_3),
    whenTrue: and(
      a!isNullOrEmpty(ri!arreglo_3),
      and(
        a!isNotNullOrEmpty(ri!arreglo_1),
        a!isNotNullOrEmpty(ri!arreglo_2)
      )
    ),
    then: difference(ri!arreglo_1, ri!arreglo_2),
    default: difference(ri!arreglo_1, ri!arreglo_2, ri!arreglo_3)
  ),
  local!devolverMatch
)</definition>
