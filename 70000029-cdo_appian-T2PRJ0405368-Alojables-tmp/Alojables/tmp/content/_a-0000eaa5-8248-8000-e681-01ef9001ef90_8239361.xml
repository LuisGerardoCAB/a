<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000eb53-0a0e-8000-e689-01ef9001ef90_9386754</versionUuid>
    <rule>
        <name>SCDO_COMPROBAR_CONTRATOS_EXISTEN_DOCUMENTACION</name>
        <uuid>_a-0000eaa5-8248-8000-e681-01ef9001ef90_8239361</uuid>
        <description>Comprueba que todos los contratos del excel estén en la tabla documentación</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4829216</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!estan: if(
    a!isNotNullOrEmpty(ri!contratos),
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!contratos,
      expression: if(
        a!isNotNullOrEmpty(
          #"_a-0000e8b2-d897-8000-e5f1-01ef9001ef90_2372887"(
            contrato: fv!item,
            iddocumento: ri!iddocumento
          ).data
        ),
        true,
        null
      )
    ),
    null
  ),
  local!devolver: reject(fn!isnull, a!flatten(local!estan)),
  if(
    and(length(local!devolver)&gt;0,length(local!devolver)=length(ri!contratos)),
    true,
    false
  
  )

)</definition>
        <namedTypedValue>
            <name>contratos</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>iddocumento</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
