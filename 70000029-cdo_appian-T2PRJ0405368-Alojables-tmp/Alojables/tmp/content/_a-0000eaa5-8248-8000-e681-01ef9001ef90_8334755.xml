<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000eb53-0a0e-8000-e689-01ef9001ef90_9386527</versionUuid>
    <rule>
        <name>SCDO_BUSCAR_ID_REPORTE_EN_ARREGLO</name>
        <uuid>_a-0000eaa5-8248-8000-e681-01ef9001ef90_8334755</uuid>
        <description>Regla de expresión auxiliar que busca el id del reporte insertado según imágenes pendientes para insertar en la tabla digitalizados</description>
        <parentUuid>_a-0000e9a3-ab27-8000-e63d-01ef9001ef90_4829216</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!busqueda: #"SYSTEM_SYSRULES_forEach"(
    items: ri!reportes,
    expression: if(
      and(
        fv!item.operacion = ri!imagen_pendiente.deal,
        fv!item.contrato = ri!imagen_pendiente.contrato,
        fv!item.usuario = ri!imagen_pendiente.usuario
      ),
      fv!item,
      null
    )
  ),
  local!devolver: a!flatten(reject(fn!isnull, local!busqueda)),
 local!final: if(a!isNotNullOrEmpty(local!devolver),
  local!devolver[1],
  null
  ),
  local!final
)</definition>
        <namedTypedValue>
            <name>imagen_pendiente</name>
            <type>
                <name>cdo_imagenes_pendientes</name>
                <namespace>urn:com:appian:types</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>reportes</name>
            <type>
                <name>cdo_reportes?list</name>
                <namespace>urn:com:appian:types</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
