        </ns2:Metadata>
      </xsd:appinfo>
      <xsd:documentation><![CDATA[CDT to create o modify a user in Appian]]></xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="username" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:appinfo source="appian.jpa">@Column(length=255)</xsd:appinfo>
